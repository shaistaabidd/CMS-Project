<%=error_messages_for(f.object)%>

<table summary="Subject for fields">
  <tr>
    <th>Name</th>
    <td><%= f.text_field(:name)%></td>
  </tr>
  
  <tr>
    <th>Visible</th>
    <td><%= f.check_box (:visible)%></td>
  </tr>
  <tr>
    <th>Created</th>
    <td><%= f.datetime_select(:created_at, order: [:day, :month, :year])%></td>
    
  </tr>
  <tr>
    <th>Zip Code</th>
    <td><%= f.text_field(:zip_code)%></td>
    
  </tr>
</table>